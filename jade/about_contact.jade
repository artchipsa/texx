	body
		.glogal-wrapper
			include modules/header.jade
			.assets-preload
				include ../img/assets.svg
			.overflow-wrapper
				.main-content.inner
					a(href="#").scroll-up.scroll-control
						svg(viewBox="0 0 100 100", class="svgico arrow-icon")
							use(xlink:href="#arrow-left")
					a(href="#").scroll-down.scroll-control
						svg(viewBox="0 0 100 100", class="svgico arrow-icon")
							use(xlink:href="#arrow-left")
					.navigation-bar
						.section-nav
							.top
								.side-title Напишите нам
								a.back-link(href="#")
									svg(viewBox="0 0 100 100", class="svgico arrow-icon")
										use(xlink:href="#arrow-left")
									span Назад
							.bottom
								.mobile-scroll
									ul.mobile-scroll-container
										li.mobile-scroll-elem
											a(href="/about.html") Компания «ТЭККС»
										li.mobile-scroll-elem
											a(href="/about_adv.html") Преимущества
										li.mobile-scroll-elem
											a(href="/about_partner.html") Партнерская сеть
										li.mobile-scroll-elem
											a(href="/about_recommend.html") Нас рекомендуют
										li.mobile-scroll-elem
											a(href="/about_license.html") Сертификаты, лицензии
										li.active.mobile-scroll-elem
											a(href="/about_contact.html") Напишите нам
						.page-nav
							a.back-link(href="#")
								svg(viewBox="0 0 100 100", class="svgico arrow-icon")
									use(xlink:href="#arrow-left")
								span Назад
					.scrolled-content
						section
							.contacts-block
								h2 Напишите нам
								form
									.form-group
										input.required(type="text", name="", placeholder="Контактное лицо *")
									.form-group
										input.required(type="text", name="", placeholder="Телефон *")
									.form-group
										input(type="text", name="", placeholder="Электронная почта")
									.form-group
										textarea(name="", placeholder="Текст вопроса")
									.form-group
										input.btn.default(type="submit", value="Отправить")


							


