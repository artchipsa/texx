doctype
html
	head
		meta(charset="UTF-8")
		meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no")
		title Texx
		link(rel="stylesheet", href="css/bootstrap.min.css")
		link(rel="stylesheet", href="css/bootstrap-theme.min.css")
		link(rel="stylesheet", href="css/owl.carousel.min.css")
		link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css")
		link(rel="stylesheet", href="css/jquery.mCustomScrollbar.min.css")
		link(rel="stylesheet", href="css/ion.rangeSlider.css")
		link(rel="stylesheet", href="css/style.css")
	body
		.glogal-wrapper
			include modules/header.jade
			.assets-preload
				include ../img/assets.svg
			.overflow-wrapper
				.main-content.inner
					a(href="#").scroll-up.scroll-control
						svg(viewBox="0 0 100 100", class="svgico arrow-icon")
							use(xlink:href="#arrow-left")
					a(href="#").scroll-down.scroll-control
						svg(viewBox="0 0 100 100", class="svgico arrow-icon")
							use(xlink:href="#arrow-left")
					.navigation-bar
						.section-nav
							.top
								.side-title Калькулятор
								a.back-link(href="#")
									svg(viewBox="0 0 100 100", class="svgico arrow-icon")
										use(xlink:href="#arrow-left")
									span Назад
							.bottom
								.mobile-scroll
									ul.mobile-scroll-container
										li.mobile-scroll-elem
											a(href="#") Возможности системы
										li.mobile-scroll-elem
											a(href="#") Программное обеспечение
										li.active.mobile-scroll-elem
											a(href="#") Калькулятор
						.page-nav
							a.back-link(href="#")
								svg(viewBox="0 0 100 100", class="svgico arrow-icon")
									use(xlink:href="#arrow-left")
								span Назад
					.scrolled-content
						section.calc
							h2 Калькулятор эффективности
							.calc-sliders
								.calc-row
									.row-item
										.name Количество единиц автотранспорта
									.row-item
										input(type="text")#amount
									.row-item
										.value
								.calc-row
									.row-item
										.name Средний пробег ТС в месяц
									.row-item
										input(type="text")#run
									.row-item
										.value
								.calc-row
									.row-item
										.name Стоимость литра бензина для вашей компании
									.row-item
										input(type="text")#cost
									.row-item
										.value
								.calc-row
									.row-item
										.name Средний расход топлива
									.row-item
										input(type="text")#drain
									.row-item
										.value
							.calc-economy
								.item
									span Экономия из приписанных пробегов составит:
									.value#run_economy
								.item
									span Экономия на контроле расхода топлива ТС:
									.value#sell_economy
								.item
									span Экономия за счет оптимизации маршрутов:
									.value#opti_economy
							.calc-all
								.calc-text
									span Итоговая экономия со всего автопарка
								.value#all_economy
						section
							h2 График окупаемости
							.chart-block
								#chart
							.calc-economy
								.item
									span Вложения разовые
									.value#once_budget
								.item
									span Вложения ежемесячные
									.value#every_month_budget
								.item
									span Примерный срок окупаемости
									.value#payback
						section
							.end-block
								h2 Начните свой проект с нами уже сегодня!
				.feedback
					.name Обратная связь
					a(href="").link.next-main-link
						svg(viewBox="0 0 100 100", class="svgico arrow-icon")
							use(xlink:href="#arrow-left")
					.helper-ui-text нажмите на кнопку
				include modules/bottom_modal.jade




		

		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js")
		script(src="https://use.fontawesome.com/856283dff2.js")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js")
		script(src="js/bootstrap.min.js")
		script(src="js/owl.carousel.min.js")
		script(src="js/hammer.js")
		script(src="js/jquery.hammer.js")
		script(src="js/jquery.mCustomScrollbar.concat.min.js")
		script(src="js/ion.rangeSlider.min.js")
		//- script(src="https://www.gstatic.com/charts/loader.js")
		script(src="https://code.highcharts.com/highcharts.js")
		script(src="js/app.js")